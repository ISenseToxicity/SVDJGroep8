                </graphic>
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
            </Button>
        </AnchorPane>
        <AnchorPane layoutX="727.0" layoutY="575.0" styleClass="blackground" stylesheets="@css/main.css">
            <Button fx:id="previousButton" contentDisplay="RIGHT" mnemonicParsing="false" stylesheets="@css/main.css"
                    text="Vorige">
                <graphic>
                    <Label fx:id="previousLabel" minWidth="45.0" prefHeight="12.0" prefWidth="55.0"
                           styleClass="svdj-corner" stylesheets="@css/main.css" textAlignment="RIGHT"/>
                </graphic>
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
            </Button>
        </AnchorPane>
        <Label layoutX="875.0" layoutY="98.0" styleClass="more-info-label" stylesheets="@css/main.css"
               text="Meer informatie">
            <graphic>
                <Label styleClass="more-info-corner" stylesheets="@css/main.css"/>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Label>
