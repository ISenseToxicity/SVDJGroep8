<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="727.0" minWidth="1342.0" prefHeight="900.0" prefWidth="1500.0"
      style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane layoutX="98.0" layoutY="34.0" maxHeight="727.0" maxWidth="1342.0" minHeight="727.0" minWidth="1342.0"
                prefHeight="727.0" prefWidth="1342.0">
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="1400.0"
               text="Hier Komt een vraag te staan" textAlignment="CENTER">
            <font>
                <Font name="Segoe UI Bold" size="44.0"/>
            </font>
        </Label>
        <CheckBox layoutX="325.0" layoutY="150.0" mnemonicParsing="false" prefHeight="75.0" prefWidth="750.0"
                  style="-fx-background-color: #F7F7F7;" stylesheets="@css/main.css"
                  text="Hier staat het eerst mogelijke antwoord">
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <padding>
                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
            </padding>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </CheckBox>
        <CheckBox layoutX="325.0" layoutY="250.0" mnemonicParsing="false" prefHeight="75.0" prefWidth="750.0"
                  style="-fx-background-color: #F7F7F7;" stylesheets="@css/main.css"
                  text="Hier staat het tweede mogelijke antwoord">
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <padding>
                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
            </padding>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </CheckBox>
        <CheckBox layoutX="325.0" layoutY="350.0" mnemonicParsing="false" prefHeight="75.0" prefWidth="750.0"
                  style="-fx-background-color: #F7F7F7;" stylesheets="@css/main.css"
                  text="Hier staat het derde mogelijke antwoord">
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <padding>
                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
            </padding>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </CheckBox>
        <CheckBox layoutX="325.0" layoutY="450.0" mnemonicParsing="false" prefHeight="75.0" prefWidth="750.0"
                  style="-fx-background-color: #F7F7F7;" stylesheets="@css/main.css"
                  text="Hier staat dan natuurlijk het vierde mogelijke antwoord">
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <padding>
                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
            </padding>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </CheckBox>
        <ProgressBar layoutX="325.0" layoutY="700.0" prefHeight="10.0" prefWidth="750.0" progress="0.35"
                     stylesheets="@css/main.css"/>
        <Label layoutX="665.0" layoutY="664.0" text="Voortgang">
            <font>
                <Font name="Segoe UI" size="15.0"/>
            </font>
        </Label>
        <AnchorPane layoutX="899.0" layoutY="575.0" styleClass="blackground" stylesheets="@css/main.css">
            <Button fx:id="nextButton" contentDisplay="RIGHT" mnemonicParsing="false" stylesheets="@css/main.css"
                    text="Volgende">
                <graphic>
                    <Label fx:id="nextLabel" minWidth="45.0" prefHeight="12.0" prefWidth="55.0" styleClass="svdj-corner"
